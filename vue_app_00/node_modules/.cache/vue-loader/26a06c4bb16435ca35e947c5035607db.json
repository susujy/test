 </div>\n    <!-- 淘宝logo图片 -->\n    <div class=\"imgDiv\">\n      <img class=\"logo\" src=\"../assets/tblogo.png\" alt=\"\">\n    </div>\n    <!-- 操作区域 -->\n    <div class=\"operateDiv\">\n      <!-- 账户 -->\n      <mt-field v-model=\"uname\" :placeholder=\"accountPlaceholder\"></mt-field>\n      <!-- 密码 -->\n      <mt-field  v-show=\"isUserAccount\" type=\"password\" v-model=\"upass\" class=\"myinput\" placeholder=\"请输入密码\"></mt-field>\n      <!-- 短信验证码 -->\n      <mt-field v-show=\"!isUserAccount\" placeholder=\"获取手机验证码\" class=\"myinput\"></mt-field>\n      <!-- 免费注册 -->\n      <!-- 登录和切换登录方式 -->\n      <div>\n        <mt-button size=\"large\" class=\"mybutton\" @click=\"login\">登录</mt-button>\n        <mt-button type=\"primary\" size=\"large\" class=\"mybutton\" @click=\"changeLoginMethod\">{{defaultAccountText}}</mt-button>  \n      </div>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n    data(){\n       return {\n         accountPlaceholder:\"手机号/邮箱/会员名\",\n         uname:\"\",\n         defaultAccountText:\"短信验证登录\",\n         //是否是账户的方式\n         isUserAccount:true,\n         upass:\"\"\n       }\n    },\n    created() {\n       \n    },\n    methods: {\n      login(){\n        if(!this.uname){\n          this.$messagebox(\"\",\"请输入用户名\")\n        }else if(!this.upass){\n          this.$messagebox(\"\",\"请输入密码\")\n        }\n      },\n      changeLoginMethod(){\n        if(this.isUserAccount){\n          this.accountPlaceholder=\"请输入手机号码\",\n          this.defaultAccountText=\"账户密码登录\"\n        }else{\n          this.accountPlaceholder=\"手机号/邮箱/会员名\",\n          this.defaultAccountText=\"短信验证登录\"\n        }\n        this.isUserAccount=!this.isUserAccount;\n      }\n    },\n}\n</script>\n<style scoped>\n    /* 你需要登录才能继续仿问的布局样式 */\n    .tipslogin {\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n      height: 52px;\n      background: rgb(238, 238, 238);\n      padding-left: 20px;\n      padding-right: 